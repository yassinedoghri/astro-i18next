{
  "name": "astro-i18next",
  "version": "1.0.0-alpha.1",
  "description": "Astro components for i18next",
  "scripts": {
    "test": "jest",
    "dev": "astro dev",
    "preview": "astro preview",
    "lint": "eslint --ext js,ts,mts src",
    "prettier": "prettier --check --ignore-path .gitignore .",
    "prettier:fix": "prettier --write --ignore-path .gitignore .",
    "commit": "cz",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yassinedoghri/astro-i18next.git"
  },
  "files": [
    "src/index.mts",
    "src/Trans.astro",
    "src/utils.ts"
  ],
  "exports": "./src/index.mts",
  "keywords": [
    "astro",
    "i18next",
    "i18n",
    "internationalization",
    "i10n",
    "localization"
  ],
  "author": {
    "name": "Yassine Doghri",
    "email": "yassine@doghri.fr",
    "url": "https://yassinedoghri.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yassinedoghri/astro-i18next/issues"
  },
  "homepage": "https://github.com/yassinedoghri/astro-i18next#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "astro": "^1.0.0-beta.17",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "i18next": "^21.6.16",
    "lint-staged": "^12.4.0",
    "prettier": "2.6.2",
    "prettier-plugin-astro": "^0.1.0-next.4",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.js": "eslint --ext js,ts,mts --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
